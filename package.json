{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "passwordly",
  "title": "Open Manager",
  "description": "Simple password manager",
  "icon": "key_512.png",
  "author": "Iota02",
  "owner": "iota-group",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Passwords",
      "description": "Template for submitting a form",
      "mode": "view"
    },
    {
      "name": "cryptoSettings",
      "title": "Configure Crypto",
      "description": "Open crypto configuration pane",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.63.0",
    "@reduxjs/toolkit": "^2.1.0",
    "node": "^21.2.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}